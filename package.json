{
  "name": "subporterplatform",
  "version": "1.0.0",
  "description": "Subporter is a platform to rent subscriptions to go see football matches",
  "main": "./server/bin/www",
  "private": true,
  "repository": "https://github.com/Subporter/SubporterPlatform.git",
  "scripts": {
    "start": "node ./server/bin/www",
    "dev": "set NODE_ENV=development && concurrently \"npm run tsc:w\" \"nodemon ./server/bin/www\" ",
    "prod": "set NODE_ENV=production && forever start ./server/bin/www",
    "testing": "set NODE_ENV=development && mocha ./testing/backend/testing",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "postinstall": "typings install && bower install --allow-root && gulp copy-externals"
  },
  "keywords": [
    "MEAN",
    "MongoDB",
    "ExpressJS",
    "Angular2",
    "NodeJS",
    "Subporter",
    "Platform",
    "Subscriptions",
    "Football",
    "Sports",
    "Renting"
  ],
  "author": "Alexander Leplae, Arno Vande Cappelle, Niels Bril, Simon Gemmel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Subporter/SubporterPlatform/issues"
  },
  "homepage": "https://github.com/Subporter/SubporterPlatform#readme",
